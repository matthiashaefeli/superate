<%= form_for :course, url: course_path(@course.id), method: "put" do |f| %>
  <p>
    <%= f.label :nombre %>
    <%= f.text_field :name %>
  </p>
 
  <p>
    <%= f.label :fecha_inicio %>
    <%= f.date_field :start %>
  </p>
  <p>
    <%= f.label :fecha_final %>
    <%= f.date_field :end %>
  </p>
  <!-- <p>
    <%= f.file_field :course %>
  </p> -->
 
  <p>
    <%= f.submit "Guardar Curso" %>
  </p>
<% end %>